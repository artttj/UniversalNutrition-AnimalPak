{
  "private": true,
  "dependencies": {
    "gulp": "^4.0.2",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": ">=8.9.0 <11.0.0"
  },
  "scripts": {
    "build": "npm-run-all build:prep build:inheritance -p \"build:bryantpark --pipeline\"",
    "build:production": "npm-run-all build:prep build:inheritance -p \"build:bryantpark --prod --ci --pipeline\"",
    "build:prep": "npm-run-all -p build-prep:bryantpark",
    "build-prep:bryantpark": "gulp styles:prep --gulpfile vendor/somethingdigital/magento2-theme-bryantpark/gulpfile.js",
    "build:inheritance": "gulp snowdog:inheritance --gulpfile vendor/somethingdigital/magento2-theme-bryantpark/gulpfile.js",
    "build:bryantpark": "gulp build --gulpfile --gulpfile vendor/somethingdigital/magento2-theme-bryantpark/gulpfile.js --theme=sd",
    "start": "gulp watch"
  },
  "workspaces": [
    "app/design/frontend/*/*",
    "vendor/somethingdigital/magento2-theme-bryantpark"
  ]
}
