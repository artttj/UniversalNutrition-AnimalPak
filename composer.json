{
    "name": "magento/project-enterprise-edition",
    "description": "eCommerce Platform for Growth (Enterprise Edition)",
    "type": "project",
    "version": "2.1.7",
    "license": [
        "proprietary"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/magento2-theme-blank-sass.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/magento2-frontools.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/SomethingDigital-Magento2-Theme-BryantPark.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/Magento2_SomethingDigital_Migration.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/Magento2_SomethingDigital_Bluefoot.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/composer-patcher.git"
        }
    ],
    "require": {
        "magento/product-enterprise-edition": "2.1.9",
        "composer/composer": "@alpha",
        "snowdog/frontools": "dev-develop#a88aa6",
        "snowdog/theme-blank-sass": "dev-develop",
        "somethingdigital/magento2-theme-bryantpark": "2.2.7",
        "somethingdigital/magento2-migration": "1.2.4",
        "somethingdigital/magento2-bluefoot": "dev-master",
        "gene/bluefoot": "^1.0",
        "jamescowie/composer-patcher": "^0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.0",
        "squizlabs/php_codesniffer": "1.5.3",
        "phpmd/phpmd": "@stable",
        "pdepend/pdepend": "2.2.2",
        "sjparkinson/static-review": "~4.1",
        "fabpot/php-cs-fixer": "~1.2",
        "lusitanian/oauth": "~0.3 <=0.7.0",
        "sebastian/phpcpd": "2.0.0"
    },
    "config": {
        "use-include-path": true
    },
    "autoload": {
        "psr-4": {
            "Magento\\Framework\\": "lib/internal/Magento/Framework/",
            "Magento\\Setup\\": "setup/src/Magento/Setup/",
            "Magento\\": "app/code/Magento/"
        },
        "psr-0": {
            "": "app/code/"
        },
        "files": [
            "app/etc/NonComposerComponentRegistration.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/",
            "Magento\\ToolkitFramework\\": "dev/tools/performance-toolkit/framework/Magento/ToolkitFramework/"
        }
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": "Inviqa\\Command::patch",
        "post-update-cmd": "Inviqa\\Command::patch"
    },
    "extra": {
        "magento-force": "override",
        "patches": {
            "magento-hotfix-patches": {
                "SD-001_EE_2.1.7_v1_composer": {
                    "title": "Fixes layout handle encoding in FPC",
                    "url": "composer-patches/SD-001_EE_2.1.7_v1_composer.patch",
                    "type": "patch"
                },
                "199180_MDVA-5954_BLUEFOOT_v1": {
                    "title": "Resolves Bluefoot slider loading race condition",
                    "url": "composer-patches/199180_MDVA-5954_BLUEFOOT_v1_composer.patch",
                    "type": "patch"
                },
                "520-MAGETWO_58033_2_1_2_v1": {
                    "title": "Skips disabled products in categories to workaround blank pages",
                    "url": "composer-patches/520-MAGETWO_58033_2_1_2_v1_composer.patch",
                    "type": "patch"
                },
                "MAGETWO-71363_EE_2.1.4": {
                    "title": "Resets free shipping promo on lowering quantity",
                    "url": "composer-patches/MAGETWO-71363_EE_2.1.4_composer_v1.patch",
                    "type": "patch"
                },
                "MDVA-2957_EE_2.1.2_v1": {
                    "title": "Prevents out of stock products from causing blank category pages",
                    "url": "composer-patches/MDVA-2957_EE_2.1.2_v1_composer.patch",
                    "type": "patch"
                },
                "composer-MAGETWO-62197": {
                    "title": "Fixes admin product save with > 20 custom options",
                    "url": "composer-patches/composer-MAGETWO-62197.patch",
                    "type": "patch"
                },
                "MDVA-5257_2.1.7": {
                    "title": "Fixes the cart/minicart mismatches we sometimes see",
                    "url": "composer-patches/MDVA-5257_2.1.7_composer.patch",
                    "type": "patch"
                },
                "MDVA-2105_COMPOSER_v1": {
                    "title": "Add patch to show all matching filters on a page.",
                    "url": "composer-patches/MDVA-2105_COMPOSER_v1.patch",
                    "type": "patch"
                }
            }
        }
    }
}
